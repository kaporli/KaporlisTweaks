  "manifest_version": 7,
  "target": "System-Wide",
  "inject": {},
  "patches": {
    "Toggle Transparent Footer": {
      "type": "checkbox",
      "default": "No",
      "values": {
        "Yes": {
          "footer.css": ["bigpicture", "QuickAccess", "MainMenu"]
        },
        "No": {}
      }
